<% if controller_name != "baskets" && controller_name != "orders" %>
  <% if @basket.basketItems.count > 0 %>
    <div class="basket-setter">
      <%= link_to basket_path(@basket) do %>
        <div class="basket-link">
          MY BASKET
        </div>
      <% end %>
  </div>
  <% end %>
<% end %>
